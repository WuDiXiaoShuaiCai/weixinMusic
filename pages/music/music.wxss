/* pages/music/music.wxss */
.music-list{
  display: flex;
  justify-content: space-between;
  padding:.2rem .5rem;
}
.count{
  line-height:4rem;
  width:2rem;
  text-align: center;
  color:#888;
}
.detail{
  flex:1;
  display: flex;
  justify-content: space-between;
  border-bottom:1px solid #999;
}
.title{
  display: flex;
}
.message{
  padding:0 .5rem;
  flex:1;
  width:10rem;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.name{
  font:1rem/2rem "";
}
.sub-detail{
  font:.8rem/1.5rem "";
  color:#666;
  width:10rem;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.tool{
  line-height:4rem;
  width:3rem;
  text-align: center;
}


